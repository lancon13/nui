{
    "compilerOptions": {
        "baseUrl": ".",
        "paths": {
            "@nui/ui/*": [
                "packages/ui/src/*"
            ]
        },
        // Other common compiler options
        "target": "ESNext",
        "module": "ESNext",
        "moduleResolution": "bundler",
        "lib": ["ESNext", "DOM"],
        "esModuleInterop": true,
        "strict": true,
        "skipLibCheck": true
    },
    "exclude": [
        "ui-bak/*"
    ]
}